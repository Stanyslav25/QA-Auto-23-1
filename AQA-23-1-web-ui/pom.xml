<?xml version="1.0" encoding="UTF-8"?>

<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>org.example</groupId>
        <artifactId>QA-Auto-23-1</artifactId>
        <version>1.0-SNAPSHOT</version>
    </parent>

    <artifactId>AQA-23-1-web-ui</artifactId>

    <description>Sample application generated by com.a9ski:quick-start</description>

    <url>https://github.com/Stas/AQA-23-1-web-ui</url>
    <inceptionYear>2024</inceptionYear>
    <organization>
        <name>Stas</name>
        <url>https://github.com/Stas</url>
    </organization>

    <developers>
        <developer>
            <id>Stas</id>
            <name>Stas</name>
            <email>Stas@internet.rar</email>
            <url>https://github.com/Stas</url>
        </developer>
    </developers>

    <scm>
        <url>https://github.com/Stas/AQA-23-1-web-ui</url>
        <connection>scm:git:ssh://git@github.com/Stas/AQA-23-1-web-ui.git</connection>
        <developerConnection>scm:git:ssh://git@github.com/Stas/AQA-23-1-web-ui.git</developerConnection>
        <tag>HEAD</tag>
    </scm>
    <licenses>
        <license>
            <name>apache_v2</name>
            <url>https://www.apache.org/licenses/LICENSE-2.0.txt</url>
            <distribution>repo</distribution>
            <comments>A business-friendly OSS license</comments>
        </license>
    </licenses>


    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <maven.compiler.source>17</maven.compiler.source>
        <maven.compiler.target>17</maven.compiler.target>

        <maven.test.plugins.version>3.0.0-M6</maven.test.plugins.version>
    </properties>

    <dependencies>
        <dependency>
            <groupId>junit</groupId>
            <artifactId>junit</artifactId>
            <version>3.8.1</version>
            <scope>test</scope>
        </dependency>
    </dependencies>

    <build>
        <plugins>
            <plugin>
                <artifactId>maven-surefire-plugin</artifactId>
                <version>${maven.test.plugins.version}</version>
            </plugin>
            <plugin>
                <artifactId>maven-failsafe-plugin</artifactId>
                <version>${maven.test.plugins.version}</version>
            </plugin>
        </plugins>
    </build>

    <reporting>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-surefire-report-plugin</artifactId>
                <version>2.22.2</version>
            </plugin>
        </plugins>
    </reporting>

</project>
